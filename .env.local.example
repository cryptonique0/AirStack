# Environment configuration for AirStack
# Copy to .env and fill in your values

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Network Configuration
VITE_STACKS_NETWORK=testnet
VITE_STACKS_API_URL=https://stacks-node-api.testnet.stacks.co

# Contract Addresses
VITE_TOKEN_CONTRACT_ADDRESS=
VITE_AIRDROP_MANAGER_CONTRACT=
VITE_WHITELIST_MANAGER_CONTRACT=
VITE_GOVERNANCE_CONTRACT=
VITE_VESTING_CONTRACT=

# Application Configuration
VITE_APP_NAME=AirStack
VITE_APP_URL=http://localhost:3000
